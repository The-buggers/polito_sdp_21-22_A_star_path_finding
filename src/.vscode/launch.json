{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Custom-Make-And-Launch",
            "preLaunchTask": "make",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/graphtest", 
            "args": ["../Benchmark/DIMACS_custom_format/binarydUSA-road-dBAY", "321269", "263446", "5", "h"], // input_path - source - dest - threads - heuristic(h/m)
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}